{
  "manifest_version": 2,

  "name": "GitHub PR tools",
  "description": "Easily add content in your PR form.",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'sha256-layzCFr9NNf8nS4NNtSE6FAKjTYGQjPlKWfcwyIL38o='; object-src 'self'"
  
}